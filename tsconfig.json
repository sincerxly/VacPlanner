{
    "compilerOptions": {
        "target": "ES2015",
        "jsx": "react",
        "module": "es2015",
        "moduleResolution": "bundler",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "allowJs": true,
        "lib": ["dom", "esnext"],
        "types": ["node"]
    }
}
